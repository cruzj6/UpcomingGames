            <p align="center" style="font-weight: bold; font-family: Helvetica,sans-serif; font-size: 30px">Welcome!</p>
             <div align="center" id="div_sign_in_buttons">
                <a id='button_sign_in' href="/auth/ucgames/signup" class="btn btn-info" role="button">Sign In</a>
                <a id='button_sign_up' href="/auth/ucgames/signup" class="btn btn-info" role="button">Sign Up</a>
            </div>
        </div>
        <div class="col-md-4"></div>
    </div>
    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>

